// Tính S(n) = 1^2 + 2^2 + … + n^2

#include <iostream>
using namespace std;
int sum_square(int a){
    int sum = 0;
    for (int i = 1; i <= a; i++) {
        sum += i*i;
    }
    return sum;
}

int main() {
    int n;
    cout << "Nhap n: ";
    cin >> n;
    
    cout << "Tong S(" << n << ") = " << sum_square(n);
    return 0;
}
